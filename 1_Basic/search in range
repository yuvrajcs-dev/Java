public class searchinrange {
    public static void main(String args[]){
        int arr[]={18,12,-7,3,14,28};
        int target=3;
        for(int i=1;i<5;i++){
            if(arr[i]==target){
                System.out.println(i);
                return;
            }
        }
        System.out.println("-1");
    }
}
